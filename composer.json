{
  "name": "payme-uz/payme-php-sdk",
  "version": "1.0.3",
  "description": "PHP SDK for Payme Business, Merchant Subscribe and TelegramBot API",
  "homepage": "https://payme.uz/",
  "license": "MIT",
  "keywords": [
    "payme",
    "payment",
    "php",
    "payment processing",
    "sdk",
    "api"
  ],
  "authors": [
    {
      "name": "Asrorbek",
      "email": "asrorbek0325@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "php-curl-class/php-curl-class": "^11.0",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.3.6"
  },
  "autoload": {
    "psr-4": {
      "PaymeUz\\": "src/"
    }
  },
  "autoload-dev": {
    "PaymeUz\\Test\\": "tests/"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "test": "phpunit"
  }
}
